@model IEnumerable<RedisDemo.Data.Product>
@{
     ViewData["Title"]="Distributed" ;
}

<h1>Distributed</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Url)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model) {
        <tr>
            <td>
                <small>@item.Name</small>
            </td>
            <td>
                <img src="@item.Url" alt="@item.Name" with="50" height="50"/>
            </td>
        </tr>
}
    </tbody>
</table>
